ScreenGui.Parent = CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MainWindow.Name = "MainWindow"
MainWindow.Parent = ScreenGui
MainWindow.AnchorPoint = Vector2.new(0.5, 0.5)
MainWindow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
MainWindow.BackgroundTransparency = 1.000
MainWindow.Position = UDim2.new(0.5, 0, -0.5, 0)
MainWindow.Size = UDim2.new(0.949999988, 0, 0.949999988, 0)

local ToggleKeybind = "Q"
local GUI = script.Parent
local UIS = game:GetService('UserInputService')

local tog = false

local blur = Instance.new('BlurEffect', game.Lighting)
blur.Enabled = true
blur.Size = 0

FOV = workspace.CurrentCamera

local OldFov = workspace.CurrentCamera.FieldOfView

local TS = game:GetService('TweenService')
local TI = TweenInfo

local ES = Enum.EasingStyle
local ED = Enum.EasingDirection

local MainFrame = MainWindow

UIS.InputBegan:Connect(function(key)
	if key.KeyCode == Enum.KeyCode[ToggleKeybind] then
		tog = not tog
		-- GUI.Enabled = tog
		
		if tog then 
			TS:Create(FOV, TI.new(.35, ES.Quart, ED.InOut), {FieldOfView = OldFov-30}):Play()
			
			TS:Create(blur, TI.new(.15, ES.Quad, ED.InOut), {Size = 12}):Play()  
			TS:Create(MainFrame, TI.new(.35, ES.Exponential, ED.Out), {Position = UDim2.new(0.5, 0, 0.5, 0)}):Play()
		elseif not tog then 
			TS:Create(FOV, TI.new(.35, ES.Quart, ED.InOut), {FieldOfView = OldFov}):Play() 
			
			TS:Create(blur, TI.new(.15, ES.Quad, ED.InOut), {Size = 0}):Play()
			TS:Create(MainFrame, TI.new(.15, ES.Exponential, ED.In), {Position = UDim2.new(0.5, 0, -0.5, 0)}):Play()
		end
	end
end)

