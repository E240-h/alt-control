-- Scripts:
local pinglabel = Counter_3
local cmemlabel = Counter_2
local cclabel = Counter

coroutine.wrap(function()
	while wait() do
		cclabel.Text = tostring(_G.AltsConnected) .. ' Alts'
	end
end)()

coroutine.wrap(function()
	while wait() do
		local CMEM = game:GetService("Stats"):GetTotalMemoryUsageMb()
		cmemlabel.Text = math.floor(CMEM)
	end
end)()

coroutine.wrap(function()
	while wait() do
		local PING = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
		pinglabel.Text = PING .. ' ms'
	end
end)

-- AltsButton.Client is disabled.
-- SettingsButton.Tiptool is disabled.
-- SettingsButton.Client is disabled.
-- CommandsButton.Client is disabled.

local function Close_LocalScript() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local CommandInfo = script.Parent.Parent.Parent
	
	local TS = game:GetService('TweenService')
	local TI = TweenInfo
	
	local ES = Enum.EasingStyle
	local ED = Enum.EasingDirection
	
	script.Parent.MouseButton1Click:Connect(function()
		TS:Create(CommandInfo, TI.new(.35, ES.Back, ED.InOut), {Position = UDim2.new(0.5, 0, 1, 0)}):Play()
	end)
	
end
coroutine.wrap(Close_LocalScript)()

function newCommand(text, desc)
	local Command = Instance.new("TextLabel")
	local More = Instance.new("ImageButton")

	Command.Name = "Command"
	Command.Parent = Commands
	Command.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Command.BackgroundTransparency = 1.000
	Command.Size = UDim2.new(1, 0, 0, 22)
	Command.Font = Enum.Font.GothamBold
	Command.Text = _G.Prefix .. text
	Command.TextColor3 = Color3.fromRGB(75, 75, 75)
	Command.TextSize = 16.000
	Command.TextWrapped = true
	Command.TextXAlignment = Enum.TextXAlignment.Left

	More.Name = "More"
	More.Parent = Command
	More.AnchorPoint = Vector2.new(0, 0.5)
	More.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	More.BackgroundTransparency = 1.000
	More.Position = UDim2.new(0.93599999, 0, 0.5, 0)
	More.Size = UDim2.new(0, 17, 0, 17)
	More.Image = "http://www.roblox.com/asset/?id=13247732273"
	More.ImageColor3 = Color3.fromRGB(75, 75, 75)

	local TS = game:GetService('TweenService')
	local TI = TweenInfo

	local ES = Enum.EasingStyle
	local ED = Enum.EasingDirection

	More.MouseButton1Click:Connect(function()
		CommandInfo.Title.Text = text
		CommandInfo.Desc.Text = desc
		TS:Create(CommandInfo, TI.new(.6, ES.Quint, ED.InOut), {Position = UDim2.new(0.5, 0, 0.703, 0)}):Play()
	end)
end

for i,v in ipairs(_G.CMDS) do
	newCommand(i, v)
end

function newAltCard(alt)
	local AltCard = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local AltAvatar = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local Outline = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local AvatarImage = Instance.new("ImageLabel")
	local UICorner_4 = Instance.new("UICorner")
	local Status = Instance.new("Frame")
	local UICorner_5 = Instance.new("UICorner")
	local AltNameLabel = Instance.new("TextLabel")
	local StatusLabel = Instance.new("TextLabel")
	local MoreButton = Instance.new("ImageButton")

	--Properties:

	AltCard.Name = "AltCard"
	AltCard.Parent = List
	AltCard.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	AltCard.Size = UDim2.new(0, 258, 0, 53)

	UICorner.Parent = AltCard

	AltAvatar.Name = "AltAvatar"
	AltAvatar.Parent = AltCard
	AltAvatar.AnchorPoint = Vector2.new(0, 0.5)
	AltAvatar.BackgroundColor3 = Color3.fromRGB(76, 76, 76)
	AltAvatar.Position = UDim2.new(0.0348837227, 0, 0.5, 0)
	AltAvatar.Size = UDim2.new(0, 42, 0, 42)

	UICorner_2.CornerRadius = UDim.new(1, 0)
	UICorner_2.Parent = AltAvatar

	Outline.Name = "Outline"
	Outline.Parent = AltAvatar
	Outline.AnchorPoint = Vector2.new(0.5, 0.5)
	Outline.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	Outline.Position = UDim2.new(0.5, 0, 0.5, 0)
	Outline.Size = UDim2.new(0.899999976, 0, 0.899999976, 0)

	UICorner_3.CornerRadius = UDim.new(1, 0)
	UICorner_3.Parent = Outline

	AvatarImage.Name = "AvatarImage"
	AvatarImage.Parent = Outline
	AvatarImage.AnchorPoint = Vector2.new(0.5, 0.5)
	AvatarImage.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	AvatarImage.Position = UDim2.new(0.5, 0, 0.5, 0)
	AvatarImage.Size = UDim2.new(0.899999976, 0, 0.899999976, 0)
	AvatarImage.Image = "rbxassetid://9281566377"

	UICorner_4.CornerRadius = UDim.new(1, 0)
	UICorner_4.Parent = AvatarImage

	Status.Name = "Status"
	Status.Parent = AltAvatar
	Status.AnchorPoint = Vector2.new(1, 1)
	Status.BackgroundColor3 = Color3.fromRGB(78, 143, 240)
	Status.Position = UDim2.new(1, 0, 1, 0)
	Status.Size = UDim2.new(0, 11, 0, 11)

	UICorner_5.CornerRadius = UDim.new(1, 0)
	UICorner_5.Parent = Status

	AltNameLabel.Name = "AltNameLabel"
	AltNameLabel.Parent = AltCard
	AltNameLabel.AnchorPoint = Vector2.new(0.5, 0)
	AltNameLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	AltNameLabel.BackgroundTransparency = 1.000
	AltNameLabel.Position = UDim2.new(0.540697694, 0, 0.217190728, 0)
	AltNameLabel.Size = UDim2.new(0.639534891, 0, -0.0377358496, 18)
	AltNameLabel.Font = Enum.Font.GothamBold
	AltNameLabel.Text = alt
	AltNameLabel.TextColor3 = Color3.fromRGB(240, 240, 240)
	AltNameLabel.TextSize = 20.000
	AltNameLabel.TextWrapped = true
	AltNameLabel.TextXAlignment = Enum.TextXAlignment.Left

	StatusLabel.Name = "StatusLabel"
	StatusLabel.Parent = AltCard
	StatusLabel.AnchorPoint = Vector2.new(0.5, 0)
	StatusLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	StatusLabel.BackgroundTransparency = 1.000
	StatusLabel.Position = UDim2.new(0.540697694, 0, 0.546630859, 0)
	StatusLabel.Size = UDim2.new(0.639534891, 0, -0.13896367, 18)
	StatusLabel.Font = Enum.Font.GothamBold
	StatusLabel.Text = "Status: Connecting"
	StatusLabel.TextColor3 = Color3.fromRGB(78, 143, 240)
	StatusLabel.TextSize = 15.000
	StatusLabel.TextWrapped = true
	StatusLabel.TextXAlignment = Enum.TextXAlignment.Left

	MoreButton.Name = "MoreButton"
	MoreButton.Parent = AltCard
	MoreButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	MoreButton.BackgroundTransparency = 1.000
	MoreButton.Position = UDim2.new(0.860465109, 0, 0.301886797, 0)
	MoreButton.Size = UDim2.new(0, 20, 0, 20)
	MoreButton.Visible = false
	MoreButton.Image = "rbxassetid://7072719531"
	MoreButton.ImageColor3 = Color3.fromRGB(143, 143, 143)
	
	-- Delays script until alt is connected
	repeat wait() until game.Players:FindFirstChild(alt)

	StatusLabel.Text = "Status: Connected"
	StatusLabel.TextColor3 = Color3.fromRGB(62, 212, 80)
	Status.BackgroundColor3 = Color3.fromRGB(62, 212, 80)
	
	AvatarImage.Image = game.Players:GetUserThumbnailAsync(game.Players[alt], Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
	
	game.Players.PlayerRemoving:Connect(function(plr)
		if plr.Name == alt then
			warn('Alt has disconnected: ' ..alt)
			-- Notify(alt..' Disconnected', 'User has disconnected.', 10)
			
			StatusLabel.Text = "Disconnected"
			StatusLabel.TextColor3 = Color3.fromRGB(212, 37, 40)
			Status.BackgroundColor3 = Color3.fromRGB(212, 37, 40)
		end
	end)
end

for i,v in ipairs(_G.Alts) do
	newAltCard(tostring(v))
end

wait(1)
StatsWindow.Parent = MainWindow
